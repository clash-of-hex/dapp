{
  "name": "hex-battle-game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "run-p build-asset",
    "gh-pages": "gh-pages -d dist -r $(git remote get-url gh-pages) -b main -m \"deploy ref:$(git log -n 1 --pretty=format:%h)\"",
    "dev": "webpack serve",
    "build": "run-p --max-parallel 1 build-webpack build-asset build-cp build-wasm",
    "build-asset": "pwa-asset-generator src/asset/logo.svg public --favicon --type png --padding 0 --background transparent --opaque false --log false",
    "build-cp": "cp public/*.png dist/",
    "build-wasm": "cp node_modules/nekoton-wasm/nekoton_wasm_bg.wasm dist && cp public/*.wasm dist/",
    "build-webpack": "webpack",
    "deploy": "run-p --max-parallel 1 build gh-pages",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobzistiy/hex-battle-game.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kobzistiy/hex-battle-game/issues"
  },
  "homepage": "https://github.com/kobzistiy/hex-battle-game#readme",
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@eversdk/appkit": "^0.3.2",
    "@eversdk/core": "^1.38.0",
    "@eversdk/lib-node": "^1.38.0",
    "@eversdk/lib-web": "^1.38.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.3",
    "everscale-inpage-provider": "^0.3.43",
    "everscale-standalone-client": "^2.1.1",
    "fs": "^0.0.1-security",
    "gh-pages": "^4.0.0",
    "honeycomb-grid": "^4.0.1",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.11.1"
  },
